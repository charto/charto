{
	"compileOnSave": true,
	"compilerOptions": {
		"declaration": true,
		"experimentalDecorators": true,
		"lib": [ "dom", "es5", "es2015.promise", "es2015.iterable" ],
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"noImplicitThis": true,
		"outDir": "../dist",
		"removeComments": false,
		"sourceMap": false,
		"strictNullChecks": true,
		"target": "es5",
		"types": []
	}
}
